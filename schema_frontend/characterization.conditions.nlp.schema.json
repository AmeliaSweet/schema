{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/D3TaLES/schema/reb_dev/schema_frontend/characterization.conditions.nlp.schema.json",
  "title": "nlp",
  "description": "Machine learning on redox active molecules",
  "type": "object",

  "properties": {
    "DOI": {
      "description": "DOI",
      "type": "string",
      "pattern": "10\\..+/.+"
    },
    "model": {
      "description": "Model used to generate this data",
      "type": "string"
    },
    "date_generated": {
      "description": "Date and time data was generated",
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false
}